<% if(p_requ.length !== 0) { %>

<br>
<center><h4><b style="color:blue">REQUIREMENT BY DATE DETAILS</b></h4></center>
<br>
<table style="width : 90%;" class="bor">
  <thead>
    <tr>
      <th style="padding:10px;"><center>Project Name</center></th>
      <th><center>Requirement Type</center></th>      
      <th><center>Status</center></th>
      <th><center>Measurement</center></th>
      <th><center>Approximate Value</center></th>
      <th><center>Next Follow-up Date</center></th>
      <th><center>Outside Purchase Reason</center></th>
      <th><center>Bill Number</center></th>
    </tr>
  </thead>
  <tbody>
    <% for(var i = 0 ; i < p_requ.length ; i++){ %>
      <tr>
        <td style="padding:10px;"><center><%= p_requ[i].projname %></center></td>
        <td><center><%= p_requ[i].reqtype %></center></td>
        <td><center><%= p_requ[i].st %></center></td>
        <td><center><%= p_requ[i].meas %></center></td>
        <td><center><%= p_requ[i].av %></center></td>
        <td><center><%= DateService.getFormattedDate(p_requ[i].nfudate)%></center></td>
        <td><center><%= p_requ[i].opr %></center></td>
        <td><center><%= p_requ[i].bn %></center></td>
      </tr>
    <% } %>
  </tbody>
</table>


<% } %>