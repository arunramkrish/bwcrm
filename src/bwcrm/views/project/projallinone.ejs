                align: 'left',
                valign: 'middle',
                sortable: true
            }                 

            ]

        });

    });

</script>

<script>

    function doVendor(a){

        if(a === 1){
            document.getElementById('contdiv').style.display="block";
            document.getElementById('contdiv').style.visibility="visible";
        }
        else if(a === 2){
            var selectedRows =  $('#vendortable').bootstrapTable('getSelections');
            var selectedRow = selectedRows[0];
            $("#contid1").val(selectedRow.id);
        }
      
    }

</script>

